              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "avg by (instance_id, project_id, zone, metadata_system_name) (\n  avg_over_time(\n    agent_googleapis_com:memory_percent_used{monitored_resource=\"gce_instance\", state=\"used\"}[${__interval}]\n  )\n  and on(instance_id, project_id, zone)\n  (workload_googleapis_com:sap_hanamonitoring_system_connection_total{monitored_resource=\"gce_instance\"})\n)",
                  "unitOverride": "%"
                }
              }
            ],
