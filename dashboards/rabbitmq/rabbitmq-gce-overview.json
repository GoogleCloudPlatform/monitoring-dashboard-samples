                "timeSeriesQuery": {
                  "outputFullDuration": false,
                  "prometheusQuery": "(avg by (project_id, zone, instance_name) (\n    avg_over_time(\n      compute_googleapis_com:instance_cpu_utilization{monitored_resource=\"gce_instance\"}[${__interval}]\n    )\n    and\n    on(instance_id, project_id, zone)\n    (workload_googleapis_com:rabbitmq_message_current{monitored_resource=\"gce_instance\"})\n  )\n)",
                  "unitOverride": "10^2.%"
                }
