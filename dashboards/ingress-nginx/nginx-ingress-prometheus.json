{
  "displayName": "NGINX Ingress Prometheus Overview",
  "dashboardFilters": [
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "cluster",
      "templateVariable": "Cluster"
    },
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "location",
      "templateVariable": "Location"
    },
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "namespace",
      "templateVariable": "Namespace"
    }
  ],
  "mosaicLayout": {
    "columns": 48,
    "tiles": [
      {
        "width": 16,
        "height": 8,
        "widget": {
          "title": "Controller Request Volume",
          "scorecard": {
            "blankView": {},
            "thresholds": [],
            "timeSeriesQuery": {
              "prometheusQuery": "round(sum(irate(nginx_ingress_controller_requests{${Cluster},${Location},${Namespace}}[2m])), 0.001)",
              "unitOverride": ""
            }
          }
        }
      },
      {
        "xPos": 16,
        "width": 16,
        "height": 8,
        "widget": {
          "title": "Controller Connections",
          "scorecard": {
            "sparkChartView": {
              "sparkChartType": "SPARK_LINE"
            },
            "thresholds": [],
            "timeSeriesQuery": {
              "prometheusQuery": "sum(avg_over_time(nginx_ingress_controller_nginx_process_connections{${Cluster},${Location},${Namespace}}[2m]))\n",
              "unitOverride": ""
            }
          }
        }
      },
      {
        "xPos": 32,
        "width": 16,
        "height": 8,
        "widget": {
          "title": "Config Reloads",
          "scorecard": {
            "sparkChartView": {
              "sparkChartType": "SPARK_LINE"
            },
            "thresholds": [],
            "timeSeriesQuery": {
              "prometheusQuery": "sum(nginx_ingress_controller_success{${Cluster},${Location},${Namespace}})",
              "unitOverride": ""
            }
          }
        }
      },
      {
        "yPos": 36,
        "width": 24,
        "height": 8,
        "widget": {
          "title": "Ingress Request Volume",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "breakdowns": [],
                "dimensions": [],
                "measures": [],
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "round(sum(irate(nginx_ingress_controller_requests{ingress=~\".*\",${Cluster},${Location},${Namespace}}[2m])) by (ingress), 0.001)",
                  "unitOverride": ""
                }
              }
            ],
            "thresholds": [],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "",
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "yPos": 28,
        "width": 24,
        "height": 8,
        "widget": {
          "title": "Ingress Request Rate",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "breakdowns": [],
                "dimensions": [],
                "measures": [],
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "rate(nginx_ingress_controller_requests{${Cluster},${Location},${Namespace}}[2m])",
                  "unitOverride": ""
                }
              }
            ],
            "thresholds": [],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "",
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "yPos": 44,
        "width": 24,
        "height": 8,
        "widget": {
          "title": "Network I/O Pressure",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "breakdowns": [],
                "dimensions": [],
                "measures": [],
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "(irate (nginx_ingress_controller_request_size_sum{${Cluster},${Location},${Namespace}}[2m]))",
                  "unitOverride": ""
                }
              }
            ],
            "thresholds": [],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "",
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "xPos": 24,
        "yPos": 36,
        "width": 24,
        "height": 16,
        "widget": {
          "title": "Memory Usage",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "breakdowns": [],
                "dimensions": [],
                "measures": [],
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "nginx_ingress_controller_nginx_process_resident_memory_bytes{${Cluster},${Location},${Namespace}}",
                  "unitOverride": ""
                }
              }
            ],
            "thresholds": [],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "",
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "xPos": 24,
        "yPos": 20,
        "width": 24,
        "height": 16,
        "widget": {
          "title": "CPU Usage",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "breakdowns": [],
                "dimensions": [],
                "measures": [],
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "rate (nginx_ingress_controller_nginx_process_cpu_seconds_total{${Cluster},${Location},${Namespace}}[2m])",
                  "unitOverride": ""
                }
              }
            ],
            "thresholds": [],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "",
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "yPos": 20,
        "width": 24,
        "height": 8,
        "widget": {
          "title": "Upstream Latency Seconds",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "breakdowns": [],
                "dimensions": [],
                "measures": [],
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "nginx_ingress_controller_ingress_upstream_latency_seconds_sum",
                  "unitOverride": ""
                }
              }
            ],
            "thresholds": [],
            "yAxis": {
              "label": "",
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "yPos": 8,
        "width": 8,
        "height": 8,
        "widget": {
          "title": "Active Connections",
          "scorecard": {
            "sparkChartView": {
              "sparkChartType": "SPARK_LINE"
            },
            "thresholds": [],
            "timeSeriesQuery": {
              "outputFullDuration": true,
              "prometheusQuery": "nginx_ingress_controller_nginx_process_connections{state=\"active\"}",
              "unitOverride": ""
            }
          }
        }
      },
      {
        "xPos": 40,
        "yPos": 8,
        "width": 8,
        "height": 8,
        "widget": {
          "title": "Waiting Connections",
          "scorecard": {
            "sparkChartView": {
              "sparkChartType": "SPARK_LINE"
            },
            "thresholds": [],
            "timeSeriesQuery": {
              "outputFullDuration": true,
              "prometheusQuery": "nginx_ingress_controller_nginx_process_connections{state=\"waiting\"}",
              "unitOverride": ""
            }
          }
        }
      },
      {
        "xPos": 8,
        "yPos": 8,
        "width": 8,
        "height": 8,
        "widget": {
          "title": "Accepted Connections",
          "scorecard": {
            "sparkChartView": {
              "sparkChartType": "SPARK_LINE"
            },
            "thresholds": [],
            "timeSeriesQuery": {
              "outputFullDuration": true,
              "prometheusQuery": "nginx_ingress_controller_nginx_process_connections_total{state=\"accepted\"}",
              "unitOverride": ""
            }
          }
        }
      },
      {
        "xPos": 16,
        "yPos": 8,
        "width": 8,
        "height": 8,
        "widget": {
          "title": "Dropped Connections",
          "scorecard": {
            "sparkChartView": {
              "sparkChartType": "SPARK_LINE"
            },
            "thresholds": [],
            "timeSeriesQuery": {
              "outputFullDuration": true,
              "prometheusQuery": "sum(nginx_ingress_controller_nginx_process_connections_total{state=\"accepted\"}) - sum(nginx_ingress_controller_nginx_process_connections_total{state=\"handled\"})",
              "unitOverride": ""
            }
          }
        }
      },
      {
        "xPos": 24,
        "yPos": 8,
        "width": 8,
        "height": 8,
        "widget": {
          "title": "Writing Connections",
          "scorecard": {
            "sparkChartView": {
              "sparkChartType": "SPARK_LINE"
            },
            "thresholds": [],
            "timeSeriesQuery": {
              "outputFullDuration": true,
              "prometheusQuery": "nginx_ingress_controller_nginx_process_connections{state=\"writing\"}",
              "unitOverride": ""
            }
          }
        }
      },
      {
        "xPos": 32,
        "yPos": 8,
        "width": 8,
        "height": 8,
        "widget": {
          "title": "Reading Connections",
          "scorecard": {
            "sparkChartView": {
              "sparkChartType": "SPARK_LINE"
            },
            "thresholds": [],
            "timeSeriesQuery": {
              "outputFullDuration": true,
              "prometheusQuery": "nginx_ingress_controller_nginx_process_connections{state=\"reading\"}",
              "unitOverride": ""
            }
          }
        }
      },
      {
        "yPos": 16,
        "width": 24,
        "height": 4,
        "widget": {
          "title": "Requests",
          "text": {
            "content": "",
            "format": "MARKDOWN",
            "style": {
              "backgroundColor": "#FFFFFF",
              "fontSize": "FS_LARGE",
              "horizontalAlignment": "H_LEFT",
              "padding": "P_EXTRA_SMALL",
              "pointerLocation": "POINTER_LOCATION_UNSPECIFIED",
              "textColor": "#212121",
              "verticalAlignment": "V_TOP"
            }
          }
        }
      },
      {
        "xPos": 24,
        "yPos": 16,
        "width": 24,
        "height": 4,
        "widget": {
          "title": "Resource Utilization",
          "text": {
            "content": "",
            "format": "MARKDOWN",
            "style": {
              "backgroundColor": "#FFFFFF",
              "fontSize": "FS_LARGE",
              "horizontalAlignment": "H_LEFT",
              "padding": "P_EXTRA_SMALL",
              "pointerLocation": "POINTER_LOCATION_UNSPECIFIED",
              "textColor": "#212121",
              "verticalAlignment": "V_TOP"
            }
          }
        }
      }
    ]
  },
  "labels": {}
}
