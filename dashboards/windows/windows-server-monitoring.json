                  "aggregation": {
                    "perSeriesAligner": "ALIGN_MEAN"
                  },
                  "filter": "metric.type=\"agent.googleapis.com/iis/current_connections\" resource.type=\"gce_instance\"",
                  "secondaryAggregation": {}
