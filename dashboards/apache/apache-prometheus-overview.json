{
  "displayName": "Apache Prometheus Overview",
  "dashboardFilters": [
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "cluster",
      "templateVariable": "Cluster"
    },
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "location",
      "templateVariable": "Location"
    },
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "namespace",
      "templateVariable": "Namespace"
    }
  ],
  "mosaicLayout": {
    "columns": 48,
    "tiles": [
      {
        "width": 8,
        "height": 14,
        "widget": {
          "title": "Total Connections",
          "scorecard": {
            "blankView": {},
            "thresholds": [],
            "timeSeriesQuery": {
              "outputFullDuration": true,
              "prometheusQuery": "sum(apache_connections{state=\"total\",${Cluster},${Location},${Namespace}})",
              "unitOverride": ""
            }
          }
        }
      },
      {
        "xPos": 8,
        "width": 8,
        "height": 14,
        "widget": {
          "title": "Servers Up",
          "scorecard": {
            "blankView": {},
            "thresholds": [],
            "timeSeriesQuery": {
              "outputFullDuration": true,
              "prometheusQuery": "count(apache_up{${Cluster},${Location},${Namespace}} == 1)",
              "unitOverride": ""
            }
          }
        }
      },
      {
        "xPos": 24,
        "width": 24,
        "height": 14,
        "widget": {
          "title": "Uptime (seconds)",
          "timeSeriesTable": {
            "columnSettings": [
              {
                "column": "value",
                "visible": true
              },
              {
                "column": "pod",
                "visible": true
              }
            ],
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "outputFullDuration": true,
                  "prometheusQuery": "apache_uptime_seconds_total{${Cluster},${Location},${Namespace}}"
                }
              }
            ],
            "metricVisualization": "BAR"
          }
        }
      },
      {
        "yPos": 14,
        "width": 48,
        "height": 4,
        "widget": {
          "title": "Activity",
          "text": {
            "content": "",
            "format": "MARKDOWN",
            "style": {
              "backgroundColor": "#FFFFFF",
              "fontSize": "FS_LARGE",
              "horizontalAlignment": "H_LEFT",
              "padding": "P_EXTRA_SMALL",
              "pointerLocation": "POINTER_LOCATION_UNSPECIFIED",
              "textColor": "#212121",
              "verticalAlignment": "V_TOP"
            }
          }
        }
      },
      {
        "yPos": 18,
        "width": 16,
        "height": 16,
        "widget": {
          "title": "Scoreboard",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "breakdowns": [],
                "dimensions": [],
                "measures": [],
                "plotType": "STACKED_AREA",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "apache_scoreboard{${Cluster},${Location},${Namespace}}",
                  "unitOverride": ""
                }
              }
            ],
            "thresholds": [],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "",
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "xPos": 16,
        "yPos": 18,
        "width": 16,
        "height": 16,
