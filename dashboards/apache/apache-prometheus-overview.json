{
  "category": "CUSTOM",
  "displayName": "Apache Prometheus Overview",
  "dashboardFilters": [
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "cluster",
      "templateVariable": "Cluster"
    },
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "location",
      "templateVariable": "Location"
    },
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "namespace",
      "templateVariable": "Namespace"
    }
  ],
  "mosaicLayout": {
    "columns": 48,
    "tiles": [
      {
        "width": 16,
        "height": 8,
        "widget": {
          "title": "Apache Version",
          "scorecard": {
            "blankView": {},
            "thresholds": [],
            "timeSeriesQuery": {
              "outputFullDuration": true,
              "prometheusQuery": "apache_version{${Cluster},${Location},${Namespace}}",
              "unitOverride": ""
            }
          }
        }
      },
      {
        "xPos": 16,
        "width": 16,
        "height": 8,
        "widget": {
          "title": "Server Status",
          "scorecard": {
            "thresholds": [
              {
                "color": "RED",
                "direction": "BELOW",
                "label": "",
                "value": 1
              }
            ],
            "timeSeriesQuery": {
              "outputFullDuration": true,
              "prometheusQuery": "apache_up{${Cluster},${Location},${Namespace}}",
              "unitOverride": ""
            }
          }
        }
      },
      {
        "xPos": 32,
        "width": 16,
        "height": 8,
        "widget": {
          "title": "Uptime (seconds)",
          "scorecard": {
            "blankView": {},
            "thresholds": [],
            "timeSeriesQuery": {
              "outputFullDuration": true,
              "prometheusQuery": "apache_uptime_seconds_total{${Cluster},${Location},${Namespace}}",
