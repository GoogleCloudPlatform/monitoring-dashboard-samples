      {
        "height": 4,
        "widget": {
          "title": "System Free Memory % ",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "aerospike_node_stats_system_free_mem_pct{${Cluster},${Location},${Namespace}}"
