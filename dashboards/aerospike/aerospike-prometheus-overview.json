{
  "category": "CUSTOM",
  "dashboardFilters": [
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "cluster",
      "templateVariable": "Cluster"
    },
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "location",
      "templateVariable": "Location"
    },
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "namespace",
      "templateVariable": "Namespace"
    }
  ],
  "displayName": "Aerospike Prometheus Overview",
  "labels": {},
  "mosaicLayout": {
    "columns": 12,
    "tiles": [
      {
        "height": 4,
        "widget": {
          "title": "Free Memory % ",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "aerospike_namespace_memory_free_pct{${Cluster},${Location},${Namespace}}"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "scale": "LINEAR"
            }
          }
        },
        "width": 6,
        "xPos": 0,
        "yPos": 0
      },
      {
        "height": 4,
        "widget": {
          "title": "Node Connections",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "aerospike_node_stats_client_connections{${Cluster},${Location},${Namespace}}"
                }
              },
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "aerospike_node_stats_fabric_connections{${Cluster},${Location},${Namespace}}"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "scale": "LINEAR"
            }
          }
        },
        "width": 6,
        "xPos": 6,
        "yPos": 0
      },
      {
        "height": 4,
        "widget": {
          "title": "Namespace Memory Used (bytes)",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "aerospike_namespace_memory_used_bytes{${Cluster},${Location},${Namespace}}"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "scale": "LINEAR"
            }
          }
        },
        "width": 4,
        "xPos": 0,
        "yPos": 4
      },
      {
        "height": 4,
