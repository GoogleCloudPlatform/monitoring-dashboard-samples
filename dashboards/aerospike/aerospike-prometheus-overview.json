        "width": 8,
        "height": 8,
        "widget": {
          "title": "Uptime (seconds)",
          "scorecard": {
            "blankView": {},
            "thresholds": [],
            "timeSeriesQuery": {
              "outputFullDuration": true,
              "prometheusQuery": "max(aerospike_node_stats_uptime{${Cluster},${Location},${Namespace}})",
              "unitOverride": ""
            }
          }
        }
      },
