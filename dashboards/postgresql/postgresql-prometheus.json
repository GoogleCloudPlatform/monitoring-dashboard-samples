    "tiles": [
      {
        "width": 16,
        "height": 8,
        "widget": {
          "title": "Active Connections",
          "scorecard": {
            "blankView": {},
            "thresholds": [],
            "timeSeriesQuery": {
              "outputFullDuration": true,
              "prometheusQuery": "sum(pg_stat_activity_count{state=\"active\",${Cluster},${Location},${Namespace}})",
              "unitOverride": ""
            }
          }
        }
      },
      {
        "xPos": 16,
        "width": 16,
        "height": 8,
