                "breakdowns": [],
                "dimensions": [],
                "measures": [],
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "kibana_concurrent_connections{${Cluster},${Location},${Namespace}}",
                  "unitOverride": ""
                }
              }
            ],
            "thresholds": [],
