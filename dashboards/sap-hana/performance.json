                      "perSeriesAligner": "ALIGN_MEAN"
                    },
                    "filter": "metric.type=\"custom.googleapis.com/sap_hana/schema/read/total_count\" resource.type=\"gce_instance\"",
                    "pickTimeSeriesFilter": {
                      "direction": "TOP",
                      "numTimeSeries": 3,
                      "rankingMethod": "METHOD_MEAN"
                    }
                  },
                  "unitOverride": "1"
                }
              },
              {
