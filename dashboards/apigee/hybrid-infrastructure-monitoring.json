{
  "displayName": "GKE Cluster Monitoring",
  "gridLayout": {
    "columns": "2",
    "widgets": [{
      "title": "Container - CPU cores limit utilization[95/50/5 PERCENTILE]",
      "xyChart": {
        "dataSets": [{
          "timeSeriesQuery": {
            "timeSeriesFilter": {
              "filter": "metric.type\u003d\"kubernetes.io/container/cpu/limit_utilization\" resource.type\u003d\"k8s_container\"",
              "aggregation": {
                "perSeriesAligner": "ALIGN_MEAN",
                "crossSeriesReducer": "REDUCE_PERCENTILE_95"
              },
              "secondaryAggregation": {
              }
            },
            "unitOverride": "percent"
          },
          "plotType": "LINE",
          "minAlignmentPeriod": "60s"
        }, {
          "timeSeriesQuery": {
            "timeSeriesFilter": {
              "filter": "metric.type\u003d\"kubernetes.io/container/cpu/limit_utilization\" resource.type\u003d\"k8s_container\"",
              "aggregation": {
                "perSeriesAligner": "ALIGN_MEAN",
                "crossSeriesReducer": "REDUCE_PERCENTILE_05"
              },
              "secondaryAggregation": {
              }
            },
            "unitOverride": "percent"
          },
          "plotType": "LINE",
          "minAlignmentPeriod": "60s"
        }, {
          "timeSeriesQuery": {
            "timeSeriesFilter": {
              "filter": "metric.type\u003d\"kubernetes.io/container/cpu/limit_utilization\" resource.type\u003d\"k8s_container\"",
              "aggregation": {
                "perSeriesAligner": "ALIGN_MEAN",
                "crossSeriesReducer": "REDUCE_PERCENTILE_50"
              },
              "secondaryAggregation": {
              }
            },
            "unitOverride": "percent"
          },
          "plotType": "LINE",
          "minAlignmentPeriod": "60s"
        }],
        "timeshiftDuration": "0s",
        "yAxis": {
          "label": "y1Axis",
          "scale": "LINEAR"
        },
        "chartOptions": {
          "mode": "COLOR"
        }
      }
