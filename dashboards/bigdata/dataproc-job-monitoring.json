{
  "displayName": "Dataproc Job Monitoring",
  "dashboardFilters": [],
  "labels": {},
  "mosaicLayout": {
    "columns": 48,
    "tiles": [
      {
        "height": 16,
        "width": 24,
        "widget": {
          "title": "Submitted jobs by project",
          "xyChart": {
            "chartOptions": {
              "displayHorizontal": false,
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "sum by (project_id) (\n  increase(\n    dataproc_googleapis_com:cluster_job_submitted_count{\n      monitored_resource=\"cloud_dataproc_cluster\"\n    }[${__interval}]\n  )\n)"
                }
              }
            ],
            "thresholds": [],
            "timeshiftDuration": "0s",
            "yAxis": {
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "xPos": 24,
        "height": 16,
        "width": 24,
        "widget": {
          "title": "Running jobs by project",
          "xyChart": {
            "chartOptions": {
              "displayHorizontal": false,
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "avg by (project_id) (\n  avg_over_time(\n    dataproc_googleapis_com:cluster_job_running_count{\n      monitored_resource=\"cloud_dataproc_cluster\"\n    }[${__interval}]\n  )\n)"
                }
              }
            ],
            "thresholds": [],
            "timeshiftDuration": "0s",
            "yAxis": {
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "yPos": 16,
        "height": 16,
        "width": 24,
        "widget": {
          "title": "Failed jobs by project",
          "xyChart": {
            "chartOptions": {
              "displayHorizontal": false,
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "sum by (project_id) (\n  increase(\n    dataproc_googleapis_com:cluster_job_failed_count{\n      monitored_resource=\"cloud_dataproc_cluster\"\n    }[${__interval}]\n  )\n)"
                }
              }
            ],
            "thresholds": [],
            "timeshiftDuration": "0s",
            "yAxis": {
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "yPos": 16,
        "xPos": 24,
        "height": 16,
        "width": 24,
        "widget": {
          "title": "Slow jobs (>=64s) by project",
          "xyChart": {
            "chartOptions": {
              "displayHorizontal": false,
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "(\n  sum by (project_id) (\n    increase(dataproc_googleapis_com:cluster_job_completion_time_count{monitored_resource=\"cloud_dataproc_cluster\"}[1h])\n  )\n  - \n  sum by (project_id) (\n    increase(dataproc_googleapis_com:cluster_job_completion_time_bucket{monitored_resource=\"cloud_dataproc_cluster\", le=\"64\"}[1h])\n  )\n)"
                }
              }
            ],
            "thresholds": [],
            "timeshiftDuration": "0s",
            "yAxis": {
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "yPos": 32,
        "height": 8,
        "width": 24,
        "widget": {
          "title": "Cloud Logging:",
          "id": "",
          "text": {
            "content": "[View Failed job logs](https://console.cloud.google.com/logs/query?query=resource.type%3D%22cloud_dataproc_cluster%22%0Aseverity%3DERROR%0A). You may also want to expand the time range for the logs.",
            "format": "MARKDOWN",
            "style": {
              "backgroundColor": "",
              "fontSize": "FONT_SIZE_UNSPECIFIED",
              "horizontalAlignment": "HORIZONTAL_ALIGNMENT_UNSPECIFIED",
              "padding": "PADDING_SIZE_UNSPECIFIED",
              "pointerLocation": "POINTER_LOCATION_UNSPECIFIED",
              "textColor": "",
              "verticalAlignment": "VERTICAL_ALIGNMENT_UNSPECIFIED"
            }
          }
        }
      }
    ]
  }
}
