{
  "displayName": "Anthos cluster node status",
  "gridLayout": {
    "columns": "2",
    "widgets": [
      {
        "title": "Node condition",
        "xyChart": {
          "dataSets": [
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"kubernetes.io/anthos/kube_node_status_condition\" resource.type=\"k8s_container\" metric.label.\"status\"!=\"false\"",
                  "aggregation": {
                    "perSeriesAligner": "ALIGN_MEAN",
                    "crossSeriesReducer": "REDUCE_SUM",
                    "groupByFields": [
                      "resource.label.\"project_id\"",
                      "resource.label.\"location\"",
                      "resource.label.\"cluster_name\"",
                      "metric.label.\"node\"",
                      "metric.label.\"condition\"",
                      "metric.label.\"status\""
                    ]
                  },
                  "secondaryAggregation": {}
                },
                "unitOverride": "1"
              },
              "plotType": "LINE",
              "minAlignmentPeriod": "60s"
            }
          ],
          "timeshiftDuration": "0s",
          "yAxis": {
            "label": "y1Axis",
            "scale": "LINEAR"
          },
          "chartOptions": {
            "mode": "COLOR"
          }
        }
      },
      {
        "title": "CPU usage per node per mode per CPU (s/s)",
        "xyChart": {
          "dataSets": [
            {
              "timeSeriesQuery": {
                "timeSeriesFilter": {
                  "filter": "metric.type=\"kubernetes.io/anthos/node_cpu_seconds_total\" resource.type=\"k8s_node\"",
                  "aggregation": {
                    "perSeriesAligner": "ALIGN_RATE",
                    "crossSeriesReducer": "REDUCE_SUM",
                    "groupByFields": [
                      "resource.label.\"project_id\"",
                      "resource.label.\"cluster_name\"",
                      "resource.label.\"location\"",
                      "resource.label.\"node_name\"",
                      "metric.label.\"cpu\"",
                      "metric.label.\"mode\""
                    ]
                  },
                  "secondaryAggregation": {}
                }
              },
              "plotType": "LINE",
              "minAlignmentPeriod": "60s"
            }
          ],
          "timeshiftDuration": "0s",
          "yAxis": {
            "label": "y1Axis",
            "scale": "LINEAR"
          },
          "chartOptions": {
            "mode": "COLOR"
          }
        }
      },
      {
