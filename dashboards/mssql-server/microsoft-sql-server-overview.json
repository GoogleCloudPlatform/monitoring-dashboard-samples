      {
        "height": 4,
        "widget": {
          "title": "Top 10 Queries by Execution Count",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "timeSeriesQuery": {
                  "timeSeriesQueryLanguage": "fetch generic_node\n| metric\n    'external.googleapis.com/bluemedora/generic_node/microsoft_sqlserver/query/execution_count'\n| top 10\n| align delta(1m)\n| every 1m"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "scale": "LINEAR"
            }
          }
        },
        "width": 6,
        "yPos": 27
