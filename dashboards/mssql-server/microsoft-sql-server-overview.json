            },
            "dataSets": [
              {
                "plotType": "STACKED_AREA",
                "timeSeriesQuery": {
                  "timeSeriesQueryLanguage": "fetch generic_node\n| metric\n    'external.googleapis.com/bluemedora/generic_node/microsoft_sqlserver/query/average_execution_time'\n| group_by 1m,\n    [value_average_execution_time_mean: mean(value.average_execution_time)]\n| top 10\n| every 1m"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
