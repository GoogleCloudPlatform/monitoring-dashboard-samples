              {
                "breakdowns": [],
                "dimensions": [],
                "legendTemplate": "connected (${metadata.system_labels\\.name})",
                "measures": [],
                "minAlignmentPeriod": "60s",
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "outputFullDuration": false,
                  "timeSeriesFilter": {
