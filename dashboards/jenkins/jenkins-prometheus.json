            "dataSets": [
              {
                "breakdowns": [],
                "dimensions": [],
                "measures": [],
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "(vm_memory_total_max{job=~\".*jenkins.*\",${Cluster},${Location},${Namespace}} - vm_memory_total_used{job=~\".*jenkins.*\",${Cluster},${Location},${Namespace}}) / vm_memory_total_max{job=~\".*jenkins.*\",${Cluster},${Location},${Namespace}} * 100.0",
                  "unitOverride": ""
                }
              }
            ],
            "thresholds": [],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "",
              "scale": "LINEAR"
            }
          }
        }
      },
