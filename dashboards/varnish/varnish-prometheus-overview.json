                "timeSeriesQuery": {
                  "prometheusQuery": "varnish_main_client_req_400{${Cluster},${Location},${Namespace}}"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
              "scale": "LINEAR"
