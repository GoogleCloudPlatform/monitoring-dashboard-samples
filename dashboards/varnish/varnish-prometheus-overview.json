                "timeSeriesQuery": {
                  "prometheusQuery": "varnish_main_threads{${Cluster},${Location},${Namespace}}"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "label": "y1Axis",
