          "scorecard": {
            "blankView": {},
            "thresholds": [],
            "timeSeriesQuery": {
              "outputFullDuration": true,
              "prometheusQuery": "(1 - sum(mongodb_sys_memory_MemFree_kb{${Cluster},${Location},${Namespace}}) / sum(mongodb_sys_memory_MemTotal_kb{${Cluster},${Location},${Namespace}})) * 100",
              "unitOverride": ""
            }
