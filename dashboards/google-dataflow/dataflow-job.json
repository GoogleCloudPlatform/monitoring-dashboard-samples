            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "avg_over_time(\n  dataflow_googleapis_com:job_estimated_byte_count{monitored_resource=\"dataflow_job\"}[${__interval}]\n)\n",
