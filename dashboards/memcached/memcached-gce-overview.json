      {
        "height": 4,
        "widget": {
          "title": "Operation Hit Ratio",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "timeSeriesQueryLanguage": "fetch gce_instance\n| metric 'workload.googleapis.com/memcached.operations'\n| every 1m\n| group_by [metric.operation,metric.type], \n    div(sum(if(metric.type = 'hit', val(), 0)), sum(val()))"
                }
              }
            ],
            "timeshiftDuration": "0s",
            "yAxis": {
              "scale": "LINEAR"
            }
          }
        },
        "width": 4,
        "xPos": 8,
        "yPos": 0
      },
      {
        "height": 4,
        "widget": {
          "title": "Connections Total",
          "xyChart": {
            "chartOptions": {
