      {
        "height": 16,
        "width": 16,
        "widget": {
          "title": "Disk Space Available",
          "xyChart": {
            "chartOptions": {
              "displayHorizontal": false,
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "avg by (metadata_system_name) (\n  avg_over_time(\n    workload_googleapis_com:elasticsearch_node_fs_disk_available{\n      monitored_resource=\"gce_instance\"\n    }[${__interval}]\n  )\n)",
                  "unitOverride": "By"
                }
              }
            ],
            "thresholds": [],
            "timeshiftDuration": "0s",
            "yAxis": {
              "scale": "LINEAR"
            }
          }
        }
      },
