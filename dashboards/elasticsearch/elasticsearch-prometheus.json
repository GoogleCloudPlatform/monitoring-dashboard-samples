{
  "displayName": "Elasticsearch Prometheus Overview",
  "dashboardFilters": [
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "cluster",
      "templateVariable": "Cluster"
    },
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "location",
      "templateVariable": "Location"
    },
    {
      "filterType": "RESOURCE_LABEL",
      "labelKey": "namespace",
      "templateVariable": "Namespace"
    }
  ],
  "mosaicLayout": {
    "columns": 48,
    "tiles": [
      {
        "xPos": 24,
        "yPos": 21,
        "width": 24,
        "height": 8,
        "widget": {
          "title": "Cluster Status",
          "xyChart": {
            "chartOptions": {
              "mode": "COLOR"
            },
            "dataSets": [
              {
                "breakdowns": [],
                "dimensions": [],
                "measures": [],
                "plotType": "LINE",
                "targetAxis": "Y1",
                "timeSeriesQuery": {
                  "prometheusQuery": "elasticsearch_cluster_health_status{${Cluster},${Location},${Namespace}}",
                  "unitOverride": ""
                }
              }
            ],
            "thresholds": [],
            "yAxis": {
              "label": "",
              "scale": "LINEAR"
            }
          }
        }
      },
      {
        "width": 8,
        "height": 8,
        "widget": {
          "title": "Tripped Breakers",
          "scorecard": {
            "gaugeView": {
              "lowerBound": 0,
              "upperBound": 100
            },
            "thresholds": [
              {
                "color": "RED",
                "direction": "ABOVE",
                "label": "",
                "value": 50
              },
              {
                "color": "YELLOW",
                "direction": "ABOVE",
                "label": "",
                "value": 30
              }
            ],
            "timeSeriesQuery": {
              "prometheusQuery": "count(elasticsearch_breakers_tripped{${Cluster},${Location},${Namespace}})",
              "unitOverride": ""
            }
          }
        }
      },
      {
        "xPos": 8,
        "width": 8,
        "height": 8,
        "widget": {
          "title": "CPU Avg Usage %",
          "scorecard": {
            "gaugeView": {
              "lowerBound": 0,
              "upperBound": 100
            },
            "thresholds": [
              {
                "color": "RED",
                "direction": "ABOVE",
                "label": "",
                "value": 80
              },
              {
                "color": "YELLOW",
                "direction": "ABOVE",
                "label": "",
                "value": 50
              }
            ],
            "timeSeriesQuery": {
              "prometheusQuery": "sum (elasticsearch_process_cpu_percent{${Cluster},${Location},${Namespace}} ) / count (elasticsearch_process_cpu_percent{${Cluster},${Location},${Namespace}} )",
              "unitOverride": ""
            }
          }
        }
      },
      {
