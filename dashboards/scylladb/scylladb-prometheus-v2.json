                "column": "pod",
                "visible": true
              },
              {
                "column": "shard",
                "visible": true
              },
              {
                "column": "value",
                "visible": true
              }
            ],
            "dataSets": [
              {
                "timeSeriesQuery": {
                  "prometheusQuery": "scylla_node_operation_mode{${Cluster},${Location},${Namespace}}"
                }
              }
            ],
            "metricVisualization": "BAR"
          }
        }
