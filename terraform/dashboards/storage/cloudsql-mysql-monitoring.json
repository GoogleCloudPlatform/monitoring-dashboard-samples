                    "perSeriesAligner": "ALIGN_RATE"
                  },
                  "filter": "metric.type=\"cloudsql.googleapis.com/database/mysql/sent_bytes_count\" resource.type=\"cloudsql_database\"",
                  "secondaryAggregation": {}
                },
                "unitOverride": "By"
              }
            }
          ],
          "timeshiftDuration": "0s",
          "yAxis": {
            "label": "y1Axis",
            "scale": "LINEAR"
          }
        }
      },
