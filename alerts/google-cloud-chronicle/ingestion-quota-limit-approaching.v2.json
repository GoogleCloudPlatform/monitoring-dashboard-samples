{
  "displayName": "Ingestion Quota Limit Approaching Alert Policy",
  "userLabels": {},
  "conditions": [
    {
      "displayName": "Quota Limit Approaching",
      "conditionThreshold": {"trigger":{"percent":70}},
      "conditionPrometheusQueryLanguage": {
        "duration": "3600s",
        "query": "100 * sum(rate(chronicle_googleapis_com:ingestion_log_bytes_count{monitored_resource=\"chronicle.googleapis.com/Collector\"}[5m]))/min(min_over_time(chronicle_googleapis_com:ingestion_quota_limit{monitored_resource=\"chronicle.googleapis.com/Collector\",quota_type=\"SHORT_TERM_DATA_RATE\"}[6h])) > 75",
      }
    }
  ],
  "alertStrategy": {
    "autoClose": "604800s"
  },
  "combiner": "OR",
  "enabled": true,
  "notificationChannels": []
}
