{
  "displayName": "High per-second rate quota utilization - ${LIMIT_DISPLAY_NAME}",
  "documentation": {
    "content": "Alerts when usage reaches 80%. [View and edit quotas](https://console.cloud.google.com/iam-admin/quotas?${URL_PARAMS})",
    "mimeType": "text/markdown"
  },
  "userLabels": {},
  "conditions": [
    {
      "displayName": "Usage nearing per-second rate quota limit",
      "conditionPrometheusQueryLanguage": {
        "duration": "0s",
        "evaluationInterval": "30s",
        "query": "(\n  sum by (${GROUPING}) (increase{\n    '${METRIC_USAGE_TYPE}',\n    monitored_resource='${MONITORED_RESOURCE}',\n    '${OUTER_FILTER}',\n    '${QUOTA_USAGE_FILTER}',\n    '${OPTIONAL_USAGE_QUERY}'\n  }[1s])\n  /\n  min by (${GROUPING}) ({\n    '${METRIC_LIMIT_TYPE}',\n    monitored_resource='${MONITORED_RESOURCE}',\n    '${OUTER_FILTER}',\n    '${QUOTA_LIMIT_FILTER}'\n  })\n) > 0.8"
      }
    }
  ],
  "alertStrategy": {
    "autoClose": "604800s",
    "notificationPrompts": [
      "OPENED",
      "CLOSED"
    ]
  },
  "combiner": "OR",
  "enabled": true,
  "notificationChannels": [],
  "severity": "SEVERITY_UNSPECIFIED"
}
